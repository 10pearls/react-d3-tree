{
  "name": "react-d3-tree",
  "version": "0.1.0",
  "description": "React component to create interactive D3 tree hierarchies",
  "main": "lib/react-d3-tree.min.js",
  "scripts": {
    "clean": "rimraf lib/*",
    "build": "npm run clean && webpack --progress --colors --env build",
    "dev": "npm run clean && webpack --progress --colors --watch --env dev",
    "lint": "eslint .",
    "test": "npm run lint && jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "jest --watchAll",
    "show:cov": "open coverage/lcov-report/index.html",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/util/*",
      "!src/**/*.test.{js,jsx}",
      "!playground/*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 75,
        "functions": 90,
        "lines": 90
      }
    },
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/jest/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/mocks/image.js"
    }
  },
  "dependencies": {
    "clone": "^2.1.1",
    "d3": "3.5.17",
    "react": "15.4.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.2",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "regenerator-runtime": "^0.10.5",
    "style-loader": "^0.17.0",
    "webpack": "2.4.1",
    "yargs": "7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bkrem/react-d3-tree.git"
  },
  "keywords": [
    "react",
    "d3",
    "tree",
    "component"
  ],
  "author": "Ben Kremer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bkrem/react-d3-tree/issues"
  },
  "homepage": "https://github.com/bkrem/react-d3-tree"
}
