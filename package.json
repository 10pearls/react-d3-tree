{
  "name": "react-d3-tree",
  "version": "0.1.0",
  "description": "React component to create interactive D3 tree hierarchies",
  "main": "lib/react-d3-tree.js",
  "scripts": {
    "clean": "rm -rf lib/*",
    "build": "npm run clean && webpack --progress --colors --env build",
    "dev": "npm run clean && webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "dependencies": {
    "clone": "^2.1.1",
    "d3": "3.5.17",
    "react": "15.4.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "postcss-loader": "^1.3.3",
    "react-dom": "^15.5.4",
    "style-loader": "^0.17.0",
    "webpack": "2.4.1",
    "yargs": "7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bkrem/react-d3-tree.git"
  },
  "keywords": [
    "react",
    "d3",
    "tree",
    "component"
  ],
  "author": "Ben Kremer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bkrem/react-d3-tree/issues"
  },
  "homepage": "https://github.com/bkrem/react-d3-tree"
}
